{
	"manifest_version": 2,
	"name": "LivLy",
	"version": "0.1.0",
	"description": "",

	"browser_action": {
		"default_title": "LivLy"
	},
	"chrome_url_overrides": {
		"newtab": "index.html"
	},
	
	"offline_enabled": true,
	"permissions": [ "unlimitedStorage" ],
	"short_name": "LivUp",
	"update_url": "https://clients2.google.com/service/update2/crx",
	
	"background": {
		"persistent": false,
		"scripts": [ "js/background.js" ]
	}
}
